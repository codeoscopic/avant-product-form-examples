<script src="https://cdn.jsdelivr.net/npm/jsoneditor@10.0.3/dist/jsoneditor.min.js"></script>
<script>
  function renderJsonEditor(node, jsonData, options) {
    const editor = new JSONEditor(node, {
      mode: "code",
      mainMenuBar: false,
      statusBar: false,
      navigationBar: false,
      enableSort: false,
      enableTransform: false,
      ...options,
    });
    editor.set(jsonData);
    return editor;
  }
</script>
